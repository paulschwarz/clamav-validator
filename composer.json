{
    "name": "sunspikes/clamav-validator",
    "description": "Custom Laravel 5 anti-virus validator for file uploads.",
    "keywords": [
        "laravel",
        "validator",
        "clamav",
        "virus"
    ],
    "homepage": "https://github.com/sunspikes/clamav-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Krishnaprasad MG",
            "email": "sunspikes@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ppawlas/quahog.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "xenolope/quahog": "dev-binary",
        "illuminate/support": ">=4.2",
        "illuminate/validation": ">=4.1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "dev-master",
        "scrutinizer/ocular": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Sunspikes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunspikes\\Tests\\ClamavValidator\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
